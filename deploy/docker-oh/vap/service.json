{
    "description": "A template for Horizon Project-Air service",
    "documentation": "https://github.com/TIBCOSoftware/labs-air/integration/oh/README.md",
    "sharable": "multiple",
    "org": "myorg",
    "label": "vap for amd64",
    "url": "vap",
    "version": "0.1.0",
    "arch": "amd64",
    "requiredServices": [],
    "deployment": {
        "services": {
            "cv-region-of-interest": {
                "image": "bigoyang/cv-region-of-interest:latest",
                "privileged": true,
                "network": "host",
                "environment": [
                    "MQTT_BROKER_HOST=localhost",
                    "MQTT_BROKER_PORT=1883",
                    "MQTT_DESTINATION_HOST=localhost:1883",
                    "CAMERA0_ENDPOINT=http://localhost:8080/pipelines/object_detection_cpu/1",
                    "CAMERA0_SRC=file:///home/video-analytics-serving/video-samples/grocery-test.mp4",
                    "CAMERA0_ROI_NAME=Staging",
                    "CAMERA0_CROP_TBLR=0,0,0,0"
                ],
                "binds": [],
                "entrypoint": [],
                "devices": []
            },
            "video-analytic": {
                "image": "bigoyang/video-analytics-serving-gstreamer:latest",
                "privileged": true,
                "network": "host",
                "environment": [
                    "DISPLAY=$DISPLAY"
                ],
                "binds": [
                    "/tmp/.X11-unix:/tmp/.X11-unix",
                    "/home/ubuntu/loss-detection-app/pipelines:/home/video-analytics-serving/pipelines",
                    "/home/ubuntu/loss-detection-app/models:/home/video-analytics-serving/models",
                    "/home/ubuntu/loss-detection-app/video-samples:/home/video-analytics-serving/video-samples",
                    "/home/ubuntu/loss-detection-demo:/home/demo"
                ],
                "entrypoint": [],
                "devices": [
                    "/dev/dri:/dev/dri"
                ]
            }
        }
    }
}